{
  "name": "breakout-docs",
  "version": "0.0.0-development",
  "description": "documentation for breakout cave survey visualizer",
  "main": "index.js",
  "repository": "https://github.com/jedwards1211/breakout",
  "author": "Andy Edwards",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint src pages --cache",
    "lint:fix": "eslint --fix src pages --cache",
    "lint:watch": "esw --watch src pages --cache",
    "prettier": "prettier --write **/*.js",
    "prettier:check": "prettier --list-different **/*.js",
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@octokit/rest": "^16.15.0",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "next": "^7.0.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-jss": "^8.6.1",
    "react-markdown": "^4.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.1",
    "@commitlint/config-conventional": "^7.5.0",
    "@jedwards1211/commitlint-config": "^1.0.0",
    "@jedwards1211/eslint-config": "^2.0.0",
    "@jedwards1211/eslint-config-react": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "raw-loader": "^1.0.0"
  }
}
